#백준 2108

import sys
from collections import Counter

#input 대신에 i/o 시간 줄이기 위해 readline 사용
N=int(sys.stdin.readline())
nums=[]
sum=0

for i in range(N):
    temp=int(sys.stdin.readline())
    nums.append(temp)
    sum+=temp

nums.sort()

mean=round(sum/N)


median=nums[N//2]

count=Counter(nums)
freq=count.most_common()
max_freq=freq[0][1]
modes=[num for num, cnt in freq if cnt==max_freq]
modes.sort()
mode=modes[1] if len(modes)>1 else modes[0]

range=nums[N-1]-nums[0]


print(mean)
print(median)
print(mode)
print(range)


#백준 2592 (사실상 2108의 쉬운 버젼)
import sys
from collections import Counter

nums=[]
sum=0

for i in range(10):
    temp=int(sys.stdin.readline())
    nums.append(temp)
    sum+=temp

nums.sort()

mean=round(sum/10)


count=Counter(nums)
freq=count.most_common()
max_freq=freq[0][1]
modes=[num for num, cnt in freq if cnt==max_freq]
modes.sort()
mode=modes[0]


print(mean)
print(mode)



#백준 2295
N=int(input())


nums=[]
sum_set=set()

for i in range(N):
    temp=int(input())
    nums.append(temp)

nums.sort()

for i in range(len(nums)):
    for j in range(i, N):
        sum_set.add(nums[i]+nums[j])

#시간 복잡도를 줄이기 위해 a+b=d-c 형태로 보기
for d in reversed(nums):
    for c in nums:
        if (d-c) in sum_set:
            print(d)
            exit()
